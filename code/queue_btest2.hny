import queue

const N_PUT = 2
const N_GET = 2
q = queue.Queue()

def put_test(self):
    print("call put", self)
    queue.put(?q, self)
    print("done put", self)

def get_test(self):
    print("call get", self)
    let v = queue.get(?q):
        print("done get", self, v)

for i in {1..N_PUT}:
    spawn put_test(i)
for i in {1..N_GET}:
    spawn get_test(i)
